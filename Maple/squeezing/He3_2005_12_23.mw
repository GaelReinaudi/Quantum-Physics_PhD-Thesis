champ_tournant:=[]:
for i from 0 to 9 do for j from 0 to 9 do 
expo_:='erreur_'; # erreur si aucun des cas suivant n'arrive.

if j&lt;=4 and j&gt;=1        then expo_:=e1^(j-1);fi;
if j=5 or j=6           then expo_:=e1^(j-4);fi;
if j=7                  then expo_:=e1^2*exp(t*I*(-w1));fi;
if j=8                  then expo_:=e1*exp(t*I*(-w1));fi;
if j=0                  then expo_:=e1^2;fi;
if j=9                  then expo_:=e1;fi;

expo__:='erreur__'; # erreur si aucun des cas suivant n'arrive.

if i&lt;=4 and i&gt;=1        then expo__:=e1^(i-1);fi;
if i=5 or i=6           then expo__:=e1^(i-4);fi;
if i=7                  then expo__:=e1^2*exp(t*I*(-w1));fi;
if i=8                  then expo__:=e1*exp(t*I*(-w1));fi;
if i=0                  then expo__:=e1^2;fi;
if i=9                  then expo__:=e1;fi;

expo:=simplify(expo_/expo__ , assume=(t::real,w1::real,w2::real,wF::real,whf::real));
champ_tournant:=[op(champ_tournant),r||i||j(t)=s||i||j(t)*expo]:
od;od;

# maintenant, les champs :
champ_tournant:=[op(champ_tournant),
alpha1=salpha1*exp(t*I*(-w1)),
a1(t) =sa1(t) *exp(t*I*(-w1)),        
a2(t) =sa2(t) *exp(t*I*(-w2)),        
a3(t) =sa3(t) *exp(t*I*(-w1))*e1,
ac1(t)=sac1(t)*exp(t*I*(w1)) ,        
ac2(t)=sac2(t)*exp(t*I*(w2)), 
ac3(t)=sac3(t)*exp(t*I*(w1))*e1^(-1)
,a1in =sa1in  *exp(t*I*(-w1)),        
a2in  =sa2in  *exp(t*I*(-w2)),        
a3in  =sa3in  *exp(t*I*(-w1))*e1,
ac1in=sac1in *exp(t*I*(w1)) ,        
ac2in =sac2in *exp(t*I*(w2)),         
ac3in =sac3in *exp(t*I*(w1))*e1^(-1)
]:
nops(champ_tournant);
